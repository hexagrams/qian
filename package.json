{
  "private": true,
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@alifd/next": "^1.25.27",
    "@ant-design/icons": "^4.7.0",
    "@ice/stark": "^2.7.1",
    "@ice/stark-app": "^1.5.0",
    "@ice/stark-data": "^0.1.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "antd": "^4.19.2",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.8.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.15.0",
    "f2elint": "^2.2.1",
    "typescript": "^4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  }
}
