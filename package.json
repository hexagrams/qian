{
  "private": true,
  "scripts": {
    "dev": "HMR=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^1.1.20",
    "@ant-design/pro-layout": "^6.5.0",
    "@hexagrams/plugin-ice-stark": "0.0.9",
    "@ice/stark-app": "^1.5.0",
    "antd": "^4.22.8",
    "jsoneditor": "^9.9.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.33"
  },
  "devDependencies": {
    "@types/jsoneditor": "^9.5.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.33",
    "f2elint": "^2.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
